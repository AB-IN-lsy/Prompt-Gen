# Copy this file to .env.local and fill in actual values.

# === Nacos connection (required for production code & integration tests) ===
# Address of your Nacos server, format ip:port. Example: 10.0.0.1:8848
NACOS_ENDPOINT=
# Login credential for Nacos
NACOS_USERNAME=
NACOS_PASSWORD=
# Optional overrides. Leave default if unsure.
NACOS_SCHEME=http
NACOS_CONTEXT_PATH=
NACOS_GROUP=DEFAULT_GROUP
NACOS_NAMESPACE=public

# === Backend runtime secrets ===
SERVER_PORT=9090
JWT_SECRET=
JWT_ACCESS_TTL=15m
JWT_REFRESH_TTL=168h

# === MySQL config stored in Nacos ===
# DataId of the MySQL JSON config inside Nacos
MYSQL_CONFIG_DATA_ID=mysql-config.properties
# Group where the MySQL config lives
MYSQL_CONFIG_GROUP=DEFAULT_GROUP

# === Integration-test helpers (only needed when running -tags integration) ===
# Nacos config entry to read during TestNacosGetConfig
NACOS_TEST_DATA_ID=
NACOS_TEST_GROUP=
# Direct MySQL override for TestMySQLPing (skips Nacos when filled)
MYSQL_TEST_HOST=
MYSQL_TEST_USER=
MYSQL_TEST_PASS=
MYSQL_TEST_DB=
MYSQL_TEST_PARAMS=
